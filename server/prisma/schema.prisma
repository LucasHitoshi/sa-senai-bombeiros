// MATERIAIS DESCARTAVEIS ENGLOBA
// MATERIAIS DESCARTAVEIS COM SIZES
// MATERIAIS DESCARTAVEIS SEM SIZES

model MateriaisDescartaveis {
  id                                Int                                 @id @default(autoincrement())
  MateriaisDescartaveisWithoutSizes MateriaisDescartaveisWithoutSizes[]
  MateriaisDescartaveisWithSizes    MateriaisDescartaveisWithSizes[]
  InfosHospitalares                 InfosHospitalares?                  @relation(fields: [infosHospitalaresId], references: [id])
  infosHospitalaresId               Int?
}

model MateriaisDescartaveisWithoutSizes {
  id                      Int                                     @id @default(autoincrement())
  state                   Boolean?
  name                    MateriaisDescartaveisNamesWithoutSizes?
  quantity                Int?
  MateriaisDescartaveis   MateriaisDescartaveis?                  @relation(fields: [materiaisDescartaveisId], references: [id])
  materiaisDescartaveisId Int?
}

model MateriaisDescartaveisWithSizes {
  id                      Int                                  @id @default(autoincrement())
  state                   Boolean?
  name                    MateriaisDescartaveisNamesWithSizes?
  quantity                Int?
  sizes                   Sizes[]
  MateriaisDescartaveis   MateriaisDescartaveis?               @relation(fields: [materiaisDescartaveisId], references: [id])
  materiaisDescartaveisId Int?
}

model Sizes {
  id                               Int                             @id @default(autoincrement())
  selectedSize                     String?
  entries                          String[]                        @default([" "])
  MateriaisDescartaveisWithSizes   MateriaisDescartaveisWithSizes? @relation(fields: [materiaisDescartaveisWithSizesId], references: [id])
  materiaisDescartaveisWithSizesId Int?
}

enum MateriaisDescartaveisNamesWithSizes {
  ATADURAS
  KITS
  LUVAS_DESC_PARES
  TALAS_PAP
}

enum MateriaisDescartaveisNamesWithoutSizes {
  CATETER_TP_OCULOS
  COMPRESSORA_COMUM
  MASCARA_DESC
  MANTA_ALUMINIZADA
  PAS_DO_DEA
  SONDA_DE_ASPIRACAO
  SORO_FISIOLOGICO
}

model MateriaisDeixadosNoHospital {
  id                                      Int                                       @id @default(autoincrement())
  MateriaisDeixadosNoHospitalWithoutSizes MateriaisDeixadosNoHospitalWithoutSizes[]
  MateriaisDeixadosNoHospitalWithSizes    MateriaisDeixadosNoHospitalWithSizes[]
  InfosHospitalares                       InfosHospitalares?                        @relation(fields: [infosHospitalaresId], references: [id])
  infosHospitalaresId                     Int?
}

model MateriaisDeixadosNoHospitalWithoutSizes {
  id                            Int                                 @id @default(autoincrement())
  state                         Boolean?
  name                          MateriaisHospitalNamesWithoutSizes?
  quantity                      Int?
  MateriaisDeixadosNoHospital   MateriaisDeixadosNoHospital?        @relation(fields: [materiaisDeixadosNoHospitalId], references: [id])
  materiaisDeixadosNoHospitalId Int?
}

model MateriaisDeixadosNoHospitalWithSizes {
  id                            Int                              @id @default(autoincrement())
  state                         Boolean?
  name                          MateriaisHospitalNamesWithSizes?
  quantity                      Int?
  sizes                         MSizes[]
  MateriaisDeixadosNoHospital   MateriaisDeixadosNoHospital?     @relation(fields: [materiaisDeixadosNoHospitalId], references: [id])
  materiaisDeixadosNoHospitalId Int?
}

model MSizes {
  id                                     Int                                   @id @default(autoincrement())
  selectedSize                           String?
  entries                                String[]
  MateriaisDeixadosNoHospitalWithSizes   MateriaisDeixadosNoHospitalWithSizes? @relation(fields: [materiaisDeixadosNoHospitalWithSizesId], references: [id])
  materiaisDeixadosNoHospitalWithSizesId Int?
}

enum MateriaisHospitalNamesWithSizes {
  COLAR
  KED
  TTF
}

enum MateriaisHospitalNamesWithoutSizes {
  BASE_DO_ESTABILIZA
  COXINS_ESTABILIZA
  MACA_RIGIDA
  TIRANTE_ARANHA
  TIRANTE_DE_CABECA
  CANULA
}
